{
  "name": "contracts",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "clean": "rimraf ./fhevmTemp ./artifacts ./cache ./coverage ./typechain ./coverage.json",
    "compile": "cross-env TS_NODE_TRANSPILE_ONLY=true hardhat compile",
    "deploy:localhost": "cross-env TS_NODE_TRANSPILE_ONLY=true hardhat deploy --network localhost",
    "deploy:sepolia": "cross-env TS_NODE_TRANSPILE_ONLY=true hardhat deploy --network sepolia",
    "test": "cross-env TS_NODE_TRANSPILE_ONLY=true hardhat test",
    "coverage": "cross-env SOLIDITY_COVERAGE=true hardhat coverage --solcoverjs ./.solcover.js --temp artifacts",
    "typechain": "cross-env TS_NODE_TRANSPILE_ONLY=true hardhat typechain"
  },
  "keywords": [],
  "author": "",
  "license": "BSD-3-Clause-Clear",
  "description": "ReflexProof FHEVM smart contracts",
  "devDependencies": {
    "@fhevm/hardhat-plugin": "^0.3.0-1",
    "@nomicfoundation/hardhat-chai-matchers": "^2.1.0",
    "@nomicfoundation/hardhat-ethers": "^3.1.2",
    "@nomicfoundation/hardhat-verify": "^2.1.3",
    "@typechain/ethers-v6": "^0.5.1",
    "@typechain/hardhat": "^9.1.0",
    "@types/chai": "^4.3.20",
    "@types/mocha": "^10.0.10",
    "@types/node": "^20.19.24",
    "@zama-fhe/relayer-sdk": "^0.3.0-5",
    "chai": "^4.5.0",
    "chai-as-promised": "^8.0.2",
    "cross-env": "^7.0.3",
    "hardhat": "^2.27.0",
    "hardhat-deploy": "^0.11.45",
    "hardhat-gas-reporter": "^2.3.0",
    "mocha": "^11.7.5",
    "rimraf": "^6.1.0",
    "solidity-coverage": "^0.8.16",
    "ts-node": "^10.9.2",
    "typechain": "^8.3.2",
    "typescript": "^5.9.3"
  },
  "dependencies": {
    "@fhevm/solidity": "^0.9.1",
    "@openzeppelin/contracts": "^5.4.0",
    "encrypted-types": "^0.0.4",
    "ethers": "^6.15.0"
  }
